@article{li_1907_1907,
	title = {1907: {Visual} {Tracking} with {Intermittent} {Visibility}: {Switched} {Control} {Design} and {Implementation}},
	abstract = {This paper addresses the problem of visual target tracking in scenarios where a pursuer may experience intermittent loss of visibility of the target. The design of a Switched Visual Tracker (SVT) is presented which aims to meet the competing requirements of maintaining both proximity and visibility. SVT alternates between a visual tracking mode for following the target, and a recovery mode for regaining visual contact when the target falls out of sight. We establish the stability of SVT by extending the average dwell time theorem from switched systems theory, which may be of independent interest. Our implementation of SVT on an Agilicious drone [1] illustrates its effectiveness on tracking various target trajectories: it reduces the average tracking error by up to 45\% and significantly improves visibility duration compared to a baseline algorithm. The results show that our approach effectively handles intermittent vision loss, offering enhanced robustness and adaptability for real-world autonomous missions. Additionally, we demonstrate how the stability analysis provides valuable guidance for selecting parameters, such as tracking speed and recovery distance, to optimize the SVT ’s performance.},
	language = {en},
	author = {Li, Yangge},
	year = {1907},
}

@article{jung_2342_nodate,
	title = {2342: {On} the {Evaluation} of an {Uncertain} ({UV}-based) {Relative} {Position} {Sensor} for {Cooperative} {Localization} of {Unmanned} {Aerial} {Vehicles} ({UAVs})},
	abstract = {In autonomous multi-robot systems robot-torobot localization methods can be utilized to increase the robustness and to achieve a precise and robust localization of the individuals. In this paper, we investigate on the performance of the UVDAR system, which showed good performance in forming swarm constellation or in leader-following tasks. For our assessments, we recorded a dataset in an indoor environment that was equipped with a motion capture system for accurate ground truthing. We evaluate both the absolute error of the measured relative poses and the consistency of the provided measurement uncertainty with the proposed evaluation framework. The proposed credibility analysis approach for relative pose measurements supports different error type definitions and a spline-based trajectory representation to generate C2continuous reference measurements. Both, the dataset and the evaluation framework are made available to the public.},
	language = {en},
	author = {Jung, Roland and Weiss, Stephan and Horyna, Jiri and Saska, Martin},
}

@article{li_1976_1976,
	title = {1976: {HPCalib}: {High}-{Precision} {Extrinsic} {Calibration} of {LiDAR}-{Camera} {System} {Using} {Semantic} {Segmentation}},
	abstract = {As autonomous driving technology advances, the integration of LiDAR and camera sensors is becoming increasingly crucial for precise environmental perception in automated driving systems. Accurate extrinsic calibration is essential for this integration. Recent approaches leveraging the Segment Anything Model (SAM) and point cloud consistency for LiDAR-camera calibration have made significant progress in addressing challenges such as dataset limitations and poor transferability in deep learning methods. The enhanced SAM2 model features a refined architecture, streaming memory, and more efficient attention mechanisms, which together improve mask prediction accuracy and processing speed. These precise masks are incorporated into a novel LiDAR-camera calibration framework. To reduce data complexity, we voxelize the raw point cloud, resulting in more accurate point cloud attributes. Additionally, we enhance the consistency of projected points in each mask image by conducting correlation analysis on RGB image high-level semantic feature within a consistency function. Finally, we optimize the extrinsic parameters by minimizing reprojection errors based on consistency scores, using the Levenberg-Marquardt (LM) algorithm and a crossmodal distance measurement technique based on normalized information distance. Validation on the KITTI dataset shows that our method achieves comparable accuracy, significantly advancing sensor fusion techniques for autonomous driving systems.},
	language = {en},
	author = {Li, Wei and Fan, Baojie and Li, Xiaotian and Gao, Xuan and Zhang, Zhenyu},
	year = {1976},
}

@article{peng_1589_nodate,
	title = {1589: {FSL}-{LVLM}: {Friction}-{Aware} {Safety} {Locomotion} using {Large} {Vision} {Language} {Model} in {Wheeled} {Robots}},
	abstract = {Wheeled-legged robots offer signiﬁcant mobility and versatility but face substantial challenges when operating on slippery terrains. Traditional model-based controllers for these robots assume no slipping. While reinforcement learning (RL) helps quadruped robots adapt to different surfaces, recovering from slips remains challenging, especially for systems with few contact points. Estimating the ground friction coefﬁcient is another open challenge. In this paper, we propose a novel friction-aware safety locomotion framework that integrates Large Vision Language Models (LVLMs) with a RL policy. Our approach explicitly incorporates the estimated friction coefﬁcient into the RL policy, enabling the robot to adapt its behavior in advance based on the surface type before reaching it. We introduce a Friction-From-Vision (FFV) module, which leverages LVLMs to estimate ground friction coefﬁcients, eliminating the need for large datasets and extensive training. The framework was validated on a customized wheeled inverted pendulum, and experimental results demonstrate that our framework increases the success rate in completing driving tasks by adjusting speed according to terrain type, while achieving better tracking performance compared to baseline methods. Our framework can be simply integrated with any other RL policies.},
	language = {en},
	author = {Peng, Bo and Baek, DongHoon and Wang, Qijie and Ramos, Joao},
}

@article{nyboe_588_2025,
	title = {588: {Embedded} {Robust} {Model} {Predictive} {Path} {Integral} {Control} using {Sensitivity} {Tubes} and {GPU} {Acceleration}},
	abstract = {This paper proposes a method to robustify model predictive path integral (MPPI) control by directly taking into account the effects of parameter uncertainty into the controller formulation. Leveraging the recent notion of closed-loop state sensitivity, the proposed MPPI can consider the state sensitivity against parameter mismatch as a part of the system state, and consequently exploit this additional information to address the challenge of model mismatch in sampling-based model predictive control. Using an obstacle avoidance scenario, we demonstrate the use of our approach to control an aerial robot. We present an embedded implementation of our method, utilizing parallelization of computations on a GPU. Finally, we show the increased robustness of our approach over a standard MPPI controller through hardware-in-the-loop simulations and validate its embedded real-time properties.},
	language = {en},
	author = {Nyboe, Frederik Falk and Afifi, Amr and Giordano, Paolo Robuffo and Ebeid, Emad and Franchi, Antonio},
	year = {2025},
}

@article{jenny_4029_nodate,
	title = {4029: {Comparison} of {Calibrated} {Cartesian} {Control} and {Uncalibrated} {Eye}-in-{Hand} {Visual} {Servoing} for a {Deployable} {Cable}-{Driven} {Parallel} {Robot}},
	abstract = {This paper presents the implementation and evaluation of an uncalibrated visual servoing (UVS) control strategy for a Cable-Driven Parallel Robot (CDPR) system. CDPRs are known for their large workspaces and high payloadto-weight ratios, making them ideal for various applications. However, traditional control strategies typically rely on extensive calibration processes, which are impractical in changing or unstructured environments. To overcome this limitation, we implemented UVS to control the CDPR platform by leveraging real-time visual feedback without the need for prior calibration. The system employs a three-cable CDPR equipped with an eye-in-hand camera configuration for visual tracking of ArUco markers. We conducted experiments comparing three control strategies—Cartesian, Single Point UVS, and Three Points UVS—in terms of their accuracy in tracking markers across a workspace. Our results show that the Cartesian control method exhibited the lowest error rates, while the Three Points method, though theoretically more robust, struggled with alignment accuracy. The Single Point method performed moderately well but was consistently outperformed by the Cartesian approach. These findings suggest that UVS is a viable alternative to traditional calibrated approaches, especially for applications requiring flexibility and rapid deployment in unpredictable environments. Future work will explore further refinements to the multi-point tracking methods and the integration of adaptive control techniques to improve the system's overall robustness.},
	language = {en},
	author = {Jenny, Jarrett-Scott and Marshall, Matthew},
}

@article{susbielle_pointillism_nodate,
	title = {Pointillism {Wall} {Painting} {Drone} {Using} {Bouncing} {Frequency} {Control}},
	abstract = {This study presents an new robotic airborne solution for autonomous wall painting using a pointillism technique. The proposed dot-painting drone is a quadcopter equipped with an additional forward propulsion unit and a spring-mounted painting pad. It is designed to bounce on a vertical wall in order to print dots at a controlled frequency along a predefined trajectory. A dynamic model of the system is derived and used to control accurately the bouncing frequency as well as the position of the robot. The performance of the system is assessed experimentally with indoor wall painting experiments.},
	language = {en},
	author = {Susbielle, Pierre and Hably, Ahmad and Dumon, Jonathan and Gangloff, Jacques},
}

