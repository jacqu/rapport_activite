@article{zu_bidirectional_2024,
	title = {Bidirectional {Thrust} {Multirotor} {Agile} {Flight} {Control} and {Planning}},
	abstract = {Bi-directional thrust is a potential way to improve the maneuverability limit of multirotor drones. This paper presents effective planning and control methods for bidirectional thrust multirotors. First, we propose a global-singularityfree differential ﬂatness map for bi-directional thrust multirotors based on Hopf Fiberation. Second, we propose an efﬁcient adaptive bi-directional thrust-switching strategy for trajectory-tracking controller. Third, we propose an agile ﬂight trajectory planning method for bi-directional thrust multirotor vehicles. The optimal trajectory generation for bi-directional thrust switching is achieved by leveraging MINCO’s spatialtemporal optimization capability. Finally, our method is validated through extensive simulation experiments.},
	language = {en},
	author = {Zu, Siliang and Tang, Linkai and Chang, Min and Hui, Zhe},
	year = {2024},
	keywords = {First round},
}

@article{lu_deep_2023,
	title = {Deep {Reinforcement} {Learning} {Control} of {Fully}-{Constrained} {Cable}-{Driven} {Parallel} {Robots}},
	volume = {70},
	issn = {1557-9948},
	url = {https://ieeexplore.ieee.org/document/9884990/?arnumber=9884990},
	doi = {10.1109/TIE.2022.3203763},
	abstract = {Cable-driven parallel robots (CDPRs) have complex cable dynamics and working environment uncertainties, which bring challenges to the precise control of CDPRs. This article introduces the reinforcement learning to offset the negative effect on the control performance of CDPRs resulting from the uncertainties. The problem of controller design for CDPRs in the framework of deep reinforcement learning is investigated. A learning-based control algorithm is proposed to compensate for uncertainties due to cable elasticity, mechanical friction, etc. A basic control law is given for the nominal model, and a Lyapunov-based deep reinforcement learning control law is designed. Moreover, the stability of the closed-loop tracking system under the reinforcement learning algorithm is proved. Both simulations and experiments validate the effectiveness and advantages of the proposed control algorithm.},
	number = {7},
	urldate = {2024-11-20},
	journal = {IEEE Transactions on Industrial Electronics},
	author = {Lu, Yanqi and Wu, Chengwei and Yao, Weiran and Sun, Guanghui and Liu, Jianxing and Wu, Ligang},
	month = jul,
	year = {2023},
	note = {Conference Name: IEEE Transactions on Industrial Electronics},
	keywords = {Adaptation models, Cable-driven parallel robots (CDPRs), End effectors, Heuristic algorithms, Markov processes, Parallel robots, Reinforcement learning, Uncertainty, deep reinforcement learning, parameter uncertainties},
	pages = {7194--7204},
}

@article{gao_design_2024,
	title = {Design, {Modeling}, and {MPC}-based {Control} of a {Fully} {Vectored} {Propulsion} {Underwater} {Robot}},
	abstract = {This paper presents the design and implementation of a novel autonomous underwater robot with fully vectored propulsion based on model predictive control (MPC) to rapidly respond to the position and attitude required for autonomous operation. Speciﬁcally, the mechatronic design of the eight vectordistributed thruster layout for the robot’s fully vectored propulsion is detailed, and the software architecture based on the robot operating system (ROS) is constructed. Then, the corresponding dynamics model is established by adopting the Fossen approach for the prediction and optimization of the control process. To achieve autonomous control, an MPC-based controller is designed and implemented to calculate the control input for the speciﬁed control objective. Finally, way-point tracking and trajectorytracking experiments are carried out in an indoor tank equipped with a motion capture system to validate the feasibility and effectiveness of the robot’s design and control framework. In addition, the robustness of the robot system is veriﬁed by artiﬁcially perturbing the robot in the hovering state.},
	language = {en},
	author = {Gao, Tianzhu and Luo, Yudong and Lv, Chao and Luo, Weirong and Fu, Xianping and Zhao, Na and Luo, Xi and Shen, Yantao},
	year = {2024},
	keywords = {First round},
}

@article{zhou_fixed-time_2024,
	title = {Fixed-time {Impedance} {Control} for {Uncertain} {Robotic} {Manipulators} with {Input} {Saturation} and {Unknown} {Disturbance}},
	abstract = {This letter is concerned with the force/position hybrid control of manipulator systems with unmodeled dynamics, disturbance and input saturation. The auxiliary system is used to deal with input saturation, which improves the system control performance. The command-ﬁlter backstepping method is combined with ﬁxed-time control to design the control input. A ﬁxed-time disturbance-observer is designed to compensate for the external unknown disturbance in the systems. It is shown that all the signals in the closed-loop system are ﬁxed-time bounded. The simulation study conﬁrms effectiveness of the proposed control strategy.},
	language = {en},
	author = {Zhou, Yuexin and Wang, Qing-Guo and Liu, Jiapeng and Yu, Jinpeng},
