@article{huang_novel_2024,
	title = {A {Novel} {Parallel} {Elastic} {Drive} {Unit} for {Cable}-driven {Parallel} {Robots} to {Expand} {Wrench}-feasible {Workspace}},
	abstract = {Cable-driven parallel robots (CDPRs) are a class of parallel mechanisms known for their large workspace. In practice, the cable tension range of drive unit is a critical factor for their practical workspace. In this paper, we propose a novel parallel elastic drive unit (PE-DU), where a spring and a variableradius drum are connected in parallel with the motor to generate additional passive tension on the cable. The passive tension expands and regulates the cable tension range, thereby increasing the wrench-feasible workspace (WFW). To meets the workspace requirements, this paper presents an optimal design method that optimizes the dimensional parameters of PE-DUs while ensuring the feasibility of the required wrench set throughout the desired workspace. Simulation conducted on an eight-cable CDPR demonstrates that the proposed design method of PE-DUs effectively meets workspace requirements. The implementation of PE-DUs results in a significant increase in the WFW, more than doubling the capacity of motors in conventional drive units. Moreover, with the assistance of passive tension, PEDUs can reduce energy consumption while ensuring robust tension distribution, offering another substantial improvement over conventional drive units.},
	language = {en},
	author = {Huang, Zhongshan and Liu, Zhen and Gao, Haibo and Sun, Guangyao and Deng, Zongquan},
	year = {2024},
	keywords = {Rejected},
}

@inproceedings{perez-colado_uadventure_2017,
	title = {{uAdventure}: {The} {eAdventure} reboot: {Combining} the experience of commercial gaming tools and tailored educational tools},
	shorttitle = {{uAdventure}},
	url = {https://ieeexplore.ieee.org/document/7943087/},
	doi = {10.1109/EDUCON.2017.7943087},
	abstract = {Educational games (aka serious games, SG) are powerful educational contents. However, they are costly to develop, and once developed, SGs become dependent on software and hardware combinations that may become obsolete, such as Adobe Flash or Java Applets. Addressing these problems would allow a much greater use of SGs in education. The eAdventure authoring tool, developed by the e-UCM research group, addressed high development costs, and resulted in the creation of multiple SGs in collaboration with different institutions. However, eAdventure's Java Applets have become increasingly difficult to run due to platform obsolescence. To maintain the benefits of the eAdventure platform and user base, we have created new platform called uAdventure: an SG editor built on top of the game engine Unity that allows for the creation of educational adventure games without requiring programming. Since Unity is supported on a majority of platforms (including mobile). By developing SGs with uAdventure, the games become future-proof, as they can be updated and retargeted for new platforms as required. In this sense, uAdventure improves the lifecycle of SGs by reducing both authoring and maintenance costs.},
	urldate = {2025-01-06},
	booktitle = {2017 {IEEE} {Global} {Engineering} {Education} {Conference} ({EDUCON})},
	author = {Perez-Colado, Ivan J. and Perez-Colado, Victor M. and Martínez-Ortiz, Ivan and Freire-Moran, Manuel and Fernández-Manjón, Baltasar},
	month = apr,
	year = {2017},
	note = {ISSN: 2165-9567},
	keywords = {Electronic learning, Engines, Games, Java, Programming, Tools, Unity 3D, geolocalization, learning analytics, serious games},
	pages = {1755--1762},
}

@inproceedings{perez-colado_using_2022,
	title = {Using e-{Learning} {Standards} to {Improve} {Serious} {Game} {Deployment} and {Evaluation}},
	url = {https://ieeexplore.ieee.org/document/9766573/},
	doi = {10.1109/EDUCON52537.2022.9766573},
	abstract = {The serious game industry has been a subject of interest for the educational community in the last few years. However, despite their potential benefits when compared to traditional e-learning, use of serious games in education has been hampered by difficulties, including that of deploying games in multi-platform settings and leveraging data generated by their players to improve the learning outcomes of those players. This increasing uptake requires to simplify serious games application for both teachers and developers. This paper proposes a standards-based approach to improve serious games deployment and evaluation. Building on existing and upcoming standards will allow games to be seamlessly integrated into the learning management systems that are already present in most higher education institutions. We present initial experiments that put the proposed approach into practice using two specific tools: uAdventure and SIMVA.},
	urldate = {2025-01-06},
	booktitle = {2022 {IEEE} {Global} {Engineering} {Education} {Conference} ({EDUCON})},
	author = {Pérez-Colado, Iván José and Pérez-Colado, Victor Manuel and Martinez-Ortiz, Iván and Freire, Manuel and Fernandez-Manjón, Baltasar},
	month = mar,
	year = {2022},
	note = {ISSN: 2165-9567},
	keywords = {Buildings, Conferences, Electronic learning, Games, Industries, Learning management systems, Serious games, e-learning standards, learning analytics, serious games},
	pages = {2077--2083},
}

@inproceedings{colado_using_2023,
	title = {Using {New} {AI}-{Driven} {Techniques} to {Ease} {Serious} {Games} {Authoring}},
	url = {https://ieeexplore.ieee.org/document/10343021/},
	doi = {10.1109/FIE58773.2023.10343021},
	abstract = {Serious games are videogames whose purpose goes beyond mere entertainment. However, serious games use in mainstream education is still limited. The development of serious games is an expensive and complex process that requires the participation of different experts (e.g. domain, educators, graphic artists, and programmers). We consider that new generative AI techniques can help in the prototyping of serious games by reducing and automating some of the processes involved. There is increasing evidence that generative AI techniques such as ChatGPT or GitHub Copilot can increase the productivity of writing or coding tasks respectively. In our case, both prototyping and teaching serious games are complex because of the number and diversity of tasks involved and we are currently investigating whether AI techniques can be used to improve and simplify the process. For example, ChatGPT could support the process of creating the game narrative, and other systems such as Stable Diffusion could ease the creation of some of the graphics resources (e.g., for creating more cohesive and coherent backgrounds). Automating some of the costlier processes of game prototyping can contribute to creating better products by allowing the playtesting of different options for more effective games. As the field of generative AI is in continuous change, this paper presents a working methodology to simplify the development of serious games, that has been instantiated with concrete tools. This working methodology has been piloted effectively by one student from a Master of Design for the development of a serious game and will be tested in a serious games development course. In the article we also explore how these AI techniques can be combined with a game authoring environment such as uAdventure to systematize the development of serious games. The use of Generative AI offers great potential for improving the development of serious games and needs to be further researched alongside its applications for game-based learning education.},
	urldate = {2025-01-06},
	booktitle = {2023 {IEEE} {Frontiers} in {Education} {Conference} ({FIE})},
	author = {Colado, Iván J. Pérez and Colado, Víctor M. Pérez and Morata, Antonio Calvo and Píriz, Rubén Santa Cruz and Manjón, Baltasar Fernández},
	month = oct,
	year = {2023},
	note = {ISSN: 2377-634X},
	keywords = {Chatbots, Education, Games, Graphics, Serious games, Video games, Writing, artificial intelligence, game development, serious games, serious games authoring},
	pages = {1--9},
}

